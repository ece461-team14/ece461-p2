{
    "name": "ece461_project",
    "version": "1.0.0",
    "type": "module",
    "description": "## Authors: Jackson Fair, Brendan Mclaughlin, James Murrer, and Ata Ulas Guler",
    "main": "dist/back/src/index.js",
    "homepage": ".",
    "scripts": {
        "start": "concurrently \"npm run start:back\" \"npm run start:front\"",
        "lint": "eslint . --config ./eslint.config.mjs",
        "build": "concurrently \"npm run build:back\" \"npm run build:front\"",
        "test": "concurrently \"npm run test:back\" \"npm run test:front\"",
        "eject": "react-scripts eject",
        "coverage": "tsc && nyc instrument ./dist ./instrumented && nyc jest --verbose",
        "clean": "rm -rf back/dist front/build",
        "start:back": "tsc && node dist/back/src/index.js",
        "build:back": "cd back && tsc",
        "test:back": "tsc && nyc instrument ./dist ./instrumented && jest --verbose",
        "start:front": "cd front && npm start",
        "build:front": "cd front && npm run build",
        "test:front": "cd front && npm run test"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@eslint/js": "^9.10.0",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/aws-lambda": "^8.10.145",
        "@types/express": "^4.17.1",
        "@types/jest": "^27.5.2",
        "@types/lodash": "^4.17.12",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.5.5",
        "@vercel/ncc": "^0.38.2",
        "concurrently": "^9.1.0",
        "eslint": "^8.57.1",
        "eslint-config-react": "^1.1.7",
        "eslint-config-react-app": "^7.0.1",
        "globals": "^15.9.0",
        "ncc": "^0.3.6",
        "nyc": "^17.1.0",
        "typescript": "^4.9.5",
        "typescript-eslint": "^8.4.0"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.688.0",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "commander": "^12.1.0",
        "cors": "^2.8.5",
        "express": "^4.21.1",
        "fs-extra": "^11.2.0",
        "multer": "^1.4.5-lts.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-scripts": "^5.0.1",
        "web-vitals": "^2.1.4"
    },
    "nyc": {
        "include": [
            "dist/**/*.js"
        ],
        "exclude": [
            "test/**/*.js"
        ],
        "extension": [
            ".js",
            ".mjs"
        ],
        "reporter": [
            "text",
            "html"
        ],
        "sourceMap": false,
        "instrument": true
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
